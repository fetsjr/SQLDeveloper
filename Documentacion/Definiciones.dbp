    <Texto>{\rtf1\ansi\ansicpg1252\deff0\deflang2058{\fonttbl{\f0\fnil\fcharset0 Microsoft Sans Serif;}}
\viewkind4\uc1\pard\f0\fs24 WITH XMLNAMESPACES ( &lt;XML namespace declaration item&gt;  \par
[ \{ , &lt;XML namespace declaration item&gt; \}...] )   \par
\par
&lt;XML namespace declaration item&gt; ::=  \par
&lt;xml_namespace_uri&gt; AS &lt;xml_namespace_prefix&gt;  \par
| &lt;XML default namespace declaration item&gt;  \par
&lt;xml_namespace_uri&gt; ::= &lt;character string literal&gt;  \par
\par
--------------------------------------------------------------------------------------------------------\par
\par
&lt;xml_namespace_prefix&gt; ::= &lt;identifier&gt;  \par
\par
--------------------------------------------------------------------------------------------------------\par
&lt;XML default namespace declaration item&gt; ::=  \par
DEFAULT &lt;xml_namespace_uri&gt;  \par
------------------------------------------------------------------------------------------------------------\par
\par
\par
}
</Texto>
