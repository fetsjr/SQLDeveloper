    <Texto>{\rtf1\ansi\ansicpg1252\deff0\deflang2058{\fonttbl{\f0\fnil\fcharset0 Microsoft Sans Serif;}}
\viewkind4\uc1\pard\f0\fs24 [ FOR \{ BROWSE | &lt;XML&gt; | &lt;JSON&gt;\} ]  \par
\par
&lt;XML&gt; ::=  \par
XML   \par
\{   \par
    \{ RAW [ ( 'ElementName' ) ] | AUTO \}   \par
    [   \par
        &lt;CommonDirectivesForXML&gt;   \par
        [ , \{ XMLDATA | XMLSCHEMA [ ( 'TargetNameSpaceURI' ) ] \} ]   \par
        [ , ELEMENTS [ XSINIL | ABSENT ]   \par
    ]  \par
  | EXPLICIT   \par
    [   \par
        &lt;CommonDirectivesForXML&gt;   \par
        [ , XMLDATA ]   \par
    ]  \par
  | PATH [ ( 'ElementName' ) ]   \par
    [  \par
        &lt;CommonDirectivesForXML&gt;   \par
        [ , ELEMENTS [ XSINIL | ABSENT ] ]  \par
    ]  \par
\}   \par
\par
&lt;CommonDirectivesForXML&gt; ::=   \par
[ , BINARY BASE64 ]  \par
[ , TYPE ]  \par
[ , ROOT [ ( 'RootName' ) ] ]  \par
\par
&lt;JSON&gt; ::=  \par
JSON   \par
\{   \par
    \{ AUTO | PATH \}   \par
    [   \par
        [ , ROOT [ ( 'RootName' ) ] ]  \par
        [ , INCLUDE_NULL_VALUES ]  \par
        [ , WITHOUT_ARRAY_WRAPPER ]  \par
    ]  \par
\par
\}\par
}
</Texto>
